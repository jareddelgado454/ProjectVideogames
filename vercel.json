{
    "version": 2,
    "builds": [
      {
        "src": "./api/**/*", 
        "use": "@vercel/node"
      },
      {
        "src": "./videoGamesProject/public/**/*", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "/api/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/(.*)",
        "dest": "/videoGamesProject/public/$1"
      }
    ]
  }